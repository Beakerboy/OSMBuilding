{
  "parserOptions": {
    "ecmaVersion": 2022
  },
  "env": {
    "es6": true
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-console": [
      0
    ]
  }
}
